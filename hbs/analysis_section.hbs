<div class="container p-5">
	<nav>
		<div class="nav nav-tabs" id="nav-tab" role="tablist">
            {{#each tab_navigation}}
                <button class="nav-link{{#if this.is_first}} active{{/if}}" id="nav-{{this.id}}-tab" data-bs-toggle="tab" data-bs-target="#nav-{{this.id}}" type="button" role="tab" aria-controls="nav-{{this.id}}" aria-selected="{{#if this.is_first}}true{{else}}false{{/if}}">{{this.name}}</button>
            {{/each}}
		</div>
	</nav>
    {{#each tab_content}}
	{{{this}}}
    {{/each}}
<div class="d-flex flex-row-reverse">
    <button id="btn-download-table-{{id}}" type="button" class="d-flex align-items-center btn m-1" aria-pressed="false">
        <svg class="bi opacity-50 m-1" width="15" height="15"><use href="#download"></use></svg>
        <svg class="bi opacity-50 m-1" width="15" height="15"><use href="#table"></use></svg>
    </button>
</div>
</div>
